{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins" [
    "https://example.com"
  ],
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=MyDatabase;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AuthSettings": {
    "ValidIssuer": "https://example.com",
    "ValidAudience": "https://example.com",
    "SecretKey": "my_secret_key_12345",
    "Expires": 60, // minutes
    "UserExpiresInMonth": 6 // month
  },
  "SwaggerSetting": {
    "PrefixIsEnable": true,
    "Prefix": "api"
  }
}
